@model IEnumerable<Infraestructure.Models.residencia>

@{
    ViewBag.Title = "Residencias";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear nuevo", "Create")
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.usuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.individualsNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.annio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.estado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.otherInfoDetails)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.numeroCasa)
            </th>
            <th></th>
        </tr>
    </thead>

    <tbody id="resultado">
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.usuario)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.individualsNumber)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.annio)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.estado)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.otherInfoDetails)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.numeroCasa)
                </td>
                <td></td>
                <a href="@Url.Action("Edit", "residencia", new { id = item.id })" class="btn btn-primary"><i class="fa-solid fa-pen"></i> Editar</a>
                @*@Html.ActionLink("Edit", "Edit", new { id = item.IdLibro }) |*@
                <a href="@Url.Action("Details", "residencia", new { id = item.id })" class="btn btn-primary"><i class="fa-solid fa-circle-info"></i> Detalle</a>
                @*@Html.ActionLink("Details", "Details", new { id = item.IdLibro }) |*@

            </tr>
        }
    </tbody>

</table>



